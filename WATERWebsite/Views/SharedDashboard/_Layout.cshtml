<!doctype html>
<html lang="en" data-layout="vertical" data-topbar="light" data-sidebar="dark"  data-sidebar-size="lg" data-sidebar-image="none" data-preloader="disable">
<head>
    @Html.Partial("~/Views/SharedDashboard/_title_meta.cshtml")
    @RenderSection("styles", required: false)
    @Html.Partial("~/Views/SharedDashboard/_head_css.cshtml")
</head>
<body>
    <!-- Begin page -->
    <div id="layout-wrapper">
        @Html.Partial("~/Views/SharedDashboard/_menu.cshtml")

        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">
                    @Html.Partial("~/Views/SharedDashboard/_page_title.cshtml")

                    @RenderBody()
                </div>
            </div>
            @Html.Partial("~/Views/SharedDashboard/_footer.cshtml")
        </div>

    </div>
    <!-- END layout-wrapper -->
    @Html.Partial("~/Views/SharedDashboard/_customizer.cshtml")

    @Html.Partial("~/Views/SharedDashboard/_vendor_scripts.cshtml")

    @RenderSection("scripts", required: false)
    @*<script src="~/assetsDashboard/js/app.js"></script>*@

</body>
</html>