<!-- Breadcrumbs Start -->
<div class="rs-breadcrumbs img1">
    <div class="container">
        <div class="breadcrumbs-inner">
            <h1 class="page-title">
                @Model.ProjectName
            </h1>
            <span class="sub-text">
                @Model.ProjectLocation
            </span>
            <ul class="breadcrumbs-area">
                <li title="Go to konstruk">
                    <a class="active" asp-action="index" asp-controller="Project">Projects</a>
                </li>
                <li>@Model.ProjectName</li>
            </ul>
        </div>
    </div>
</div>
<!-- Breadcrumbs End -->
