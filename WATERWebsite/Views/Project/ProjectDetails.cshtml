                        <h4 class="title">Date</h4>
                        <div class="description">
                            <p>@Model.ProjectDate</p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="project-txt mb-30">
            <h2 class="title pb-35 mb-10">Project Overview</h2>

