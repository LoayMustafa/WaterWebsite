            <div class="slider-img">
                <img src="~/imgs/slider/style3/sl1.jpg" alt="Slider">
            </div>
            <div class="slider-img">
                <img src="~/imgs/slider/style3/sl2.jpg" alt="Slider">
            </div>
            <div class="slider-img">
                <img src="~/imgs/slider/style3/sl3.jpg" alt="Slider">
            </div>
        </div>
    </div>
</div>
