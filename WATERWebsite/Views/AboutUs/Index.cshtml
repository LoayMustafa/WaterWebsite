<!-- Team Section Start -->
<div class="rs-team team-style6 pt-120 pb-205 md-pt-80 md-pb-165">
    <div class="container">
        <div class="row">
            @if (Model.Count() > 0)
            {
                @foreach (var employee in Model)
                {
                    <div class="col-lg-4 col-md-6 mb-135">
                        <div class="team-item">
                            <div class="team-inner-wrap">
                                <div class="image-wrap">
                                    <a asp-action="EmpolyeeDetails" asp-controller="Employee" asp-route-EmployeeCode="@employee.EmployeeCode"><img src="@employee.EmployeePhotoPath" alt="Team"></a>
                                </div>
                                <div class="team-content">
                                    <h3 class="team-name">
                                        <a asp-action="EmpolyeeDetails" asp-controller="Employee" asp-route-EmployeeCode="@employee.EmployeeCode">@employee.EmployeeName</a>
                                    </h3>
                                    <span class="team-title">@employee.EmployeeJob</span>
                                    <p class="team-desc"><a asp-action="EmpolyeeDetails" asp-controller="Employee" asp-route-EmployeeCode="@employee.EmployeeCode">Details</a></p>
                                    <ul class="social-icons">
                                        <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                                        <li><a href="#"><i class="fa fa-instagram"></i></a>   </li>
                                        <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                                        <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>
                }
            }
        </div>
        View All
    </div>
</div>
<!-- Team Section End -->
