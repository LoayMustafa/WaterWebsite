<!-- Services Start -->
<div id="rs-services" class="rs-services services-style3 bg5 pt-60 pb-60 md-pt-70 md-pb-80">
    <div class="ml-25">
        <div class="sec-title2 text-center mb-65 md-mb-45">
            <span class="sub-title">@DbRes.T("OurStory", "Resources", language)</span>
            <div class="heading-border-line"></div>

        </div>
        <div class="container">
            <div class="row y-middle pb-37">
                <div class="col-lg-4 md-mb-50">
                    <div class="images-part">
                        <img src="~/imgs/about/success.png" alt="success">
                    </div>
                </div>
                <div class="col-lg-8 md-pl-15">
                    <div class="sec-title">
                        <span class="sub-text">
                            <img src="~/imgs/shape-1.png" alt="Images">
                            @DbRes.T("OurStoryBrief1", "Resources", language)
                        </span>
                        <p class="desc">
                            @DbRes.T("GrowthStrategyBrief", "Resources", language)
                        </p>
                    </div>
                </div>
            </div>
            <div class="row y-middle">
                <div class="col-lg-7 md-pl-15">
                    <div class="sec-title">
                        <h2 class="title pb-20">
                            @DbRes.T("GrowthStrategy", "Resources", language):
                        </h2>
                        <p class="desc pb-37">
                            @DbRes.T("GrowthStrategyBrief1", "Resources", language)<br />
                            @DbRes.T("GrowthStrategyBrief2", "Resources", language)

                        </p>
                    </div>
                </div>
                <div class="col-lg-5 md-mb-50">
                    <div class="images-part">
                        <img src="~/imgs/about/growth.jpg" alt="growth">
                    </div>
                </div>
                <div class="col-lg-5 md-mb-50">
                    <div class="images-part">
                        <img src="~/imgs/about/ceo.jpg" alt="ceo">
                    </div>
                </div>
                <div class="col-lg-7 md-pl-15">
                    <div class="sec-title">
                        <h2 class="title pb-20">
                            @DbRes.T("CeoWord", "Resources", language):
                        </h2>
                        <p class="desc pb-37">
                            @DbRes.T("CeoWordBrief", "Resources", language)<br />
                            @DbRes.T("CeoWordBrief1", "Resources", language)<br />
                            @DbRes.T("CeoWordBrief2", "Resources", language)
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
