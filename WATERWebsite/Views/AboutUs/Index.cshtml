@using Westwind.Globalization;
@model IEnumerable<WATERWebsite.Core.ViewModels.AboutUsViewModels.AboutUsTeamViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var language = "en";
    if (Context.Session.GetString("lang") != null)
    {
        language = Context.Session.GetString("lang");
    }
}
<link rel="stylesheet" type="text/css" href="~/assets/css/style.css" />

<!-- Breadcrumbs Start -->
<div class="rs-breadcrumbs img1">
    <div class="container">
        <div class="breadcrumbs-inner">
            <h1 class="page-title">
                @DbRes.T("AboutUS", "Resources", language)
            </h1>
            <ul class="breadcrumbs-area">
                <li title="Home">
                    <a class="active" href="/">@DbRes.T("Home", "Resources", language)</a>
                </li>
                <li>@DbRes.T("AboutUS", "Resources", language)</li>
            </ul>
        </div>
    </div>
</div>
<!-- Breadcrumbs End -->
<!-- About Start -->
<div class="rs-about about-style1 bg1 pt-60 pb-60 md-pt-80 md-pb-75">
    <div class="container">
        <div class="row y-middle mb-5">
            <div class="col-lg-6 md-mb-50">
                <div class="images-part">
                    <img src="~/imgs/about/style1/who.jpg" alt="About">
                </div>
            </div>
            <div class="col-lg-6 pl-50 md-pl-15">
                <div class="sec-title">
                    <span class="sub-text">
                        <img src="~/imgs/shape-1.png" alt="Images">
                        @DbRes.T("WhoWeAre", "Resources", language)
                    </span>
                    <p class="desc pb-37">
                        @DbRes.T("WhoWeAreBrief", "Resources", language)
                    </p>
                </div>
            </div>
        </div>
        <div class="row y-middle">
            <div class="col-lg-6 pl-50 md-pl-15">
                <div class="sec-title">
                    <h2 class="title pb-20">
                        @DbRes.T("OurVision", "Resources", language):
                    </h2>
                    <p class="desc pb-37">
                        @DbRes.T("OurVisionBrief", "Resources", language)
                    </p>
                    <h2 class="title pb-20">
                        @DbRes.T("OurMission", "Resources", language):
                    </h2>
                    <p class="desc pb-37">
                        @DbRes.T("OurMissionBrief1", "Resources", language)<br />
                        @DbRes.T("OurMissionBrief2", "Resources", language)
                    </p>
                </div>
            </div>
            <div class="col-lg-6 md-mb-50">
                <div class="images-part">
                    <img src="~/imgs/about/style1/about.png" alt="About">
                </div>
            </div>
        </div>
    </div>
</div>
<!-- About End -->
